Shadow { title: "花見".to_string(), lines: vec![ Line { speaker: "narrator".to_string(), description: "えりさんとゆみさんは東京国さい大学の国さい学部でビジネスをせんこうしている大学生です。".to_string(), start: 7.42481, end: 14.609233, }, Line { speaker: "narrator".to_string(), description: "二人は英語がペラペラ話せて、りょこうをしたり、サークル活動をしたりして、".to_string(), start: 15.2653, end: 20.796533, }, Line { speaker: "narrator".to_string(), description: "大学生活をせいーぱい楽しんでいる　女子大生です。".to_string(), start: 21.1336, end: 25.418433, }, Line { speaker: "ゆみ".to_string(), description: "今日はいい天気ね。".to_string(), start: 34.2637, end: 35.785333, }, Line { speaker: "ゆみ".to_string(), description: "このごろあたたかかったり、さむかったりしているから".to_string(), start: 35.8781, end: 38.993833, }, Line { speaker: "えり".to_string(), description: "本当、　雨だったり、天気だったりしてね。".to_string(), start: 39.2703, end: 42.715633, }, Line { speaker: "えり".to_string(), description: "でも、今日はぽかぽかしてる。".to_string(), start: 43.4447, end: 45.501833, }, Line { speaker: "えり".to_string(), description: "さくらもきれいになってきたね。".to_string(), start: 46.4659, end: 48.246833, }, Line { speaker: "えり".to_string(), description: "この週まつはもうまん開だって。".to_string(), start: 48.9349, end: 51.214933, }, Line { speaker: "ゆみ".to_string(), description: "へえ、じゃ、今週花見行ったほうがいいね。".to_string(), start: 51.6874, end: 55.136033, }, Line { speaker: "えり".to_string(), description: "うん、さっそくこの金曜日に行って見る？".to_string(), start: 159.759, end: 162.985333, }, Line { speaker: "ゆみ".to_string(), description: "いいね。私、去年は大学のサークルの人と夜ざくらを見に行ったんだけど".to_string(), start: 166.967, end: 174.093333, }, Line { speaker: "えり".to_string(), description: "へー、夜ざくら。".to_string(), start: 64.9158, end: 66.844033, }, Line { speaker: "えり".to_string(), description: "私は行ったことがないんだ。".to_string(), start: 67.5299, end: 69.538533, }, Line { speaker: "えり".to_string(), description: "夜はどうだった？".to_string(), start: 70.2756, end: 71.668433, }, Line { speaker: "ゆみ".to_string(), description: "ライトアップでさくらがとってもきれいに見えたよ。".to_string(), start: 72.1037, end: 75.439133, }, Line { speaker: "ゆみ".to_string(), description: "でも、今年はバイトしてるからざんねんながら、夜ざくらは見に行けないけど。".to_string(), start: 75.7149, end: 81.038033, }, Line { speaker: "えり".to_string(), description: "そう、私も今週の夜はよていが入っているから、昼間だね。".to_string(), start: 81.6518, end: 86.969033, }, Line { speaker: "えり".to_string(), description: "どこにする？".to_string(), start: 87.49, end: 88.584733, }, Line { speaker: "ゆみ".to_string(), description: "やっぱり上野公園かな。".to_string(), start: 89.517, end: 91.857033, }, Line { speaker: "ゆみ".to_string(), description: "びじゅつ館に行ったり、動物園でパンダを見たりして、きっと楽しいよ。".to_string(), start: 92.5661, end: 97.536733, }, Line { speaker: "えり".to_string(), description: "パンダか。それもいいね。".to_string(), start: 97.9223, end: 100.327333, }, Line { speaker: "えり".to_string(), description: "ジョンさんもさそってみる？".to_string(), start: 100.914, end: 102.590333, }, Line { speaker: "えり".to_string(), description: "かれはまだ日本に来てからーども花見をしたことがないと思うんだ。".to_string(), start: 103.482, end: 108.307333, }, Line { speaker: "ゆみ".to_string(), description: "そらはいいね。".to_string(), start: 108.525, end: 109.674333, }, ], audio_url: "https://www.ccsf.edu/Departments/Language_Center/oll/japanese/ganbaroo_2/19cd17/02_dialogue_part_1.mp3".to_string(), }